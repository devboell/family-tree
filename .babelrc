{
  "plugins": ["transform-object-rest-spread"],
  "env": {
    "production": {
      "presets": [["env", { "modules": false }], "react"]
    },
    "development": {
      "presets": [["env", { "modules": false }], "react"],
      "plugins": ["react-hot-loader/babel", "transform-es2015-modules-commonjs"]
    },
    "test": {
      "presets": ["env", "react"],
      "plugins": ["transform-es2015-modules-commonjs", "istanbul"]
    }
  }
}
